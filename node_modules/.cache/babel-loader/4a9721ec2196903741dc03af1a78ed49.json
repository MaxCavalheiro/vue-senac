deProd(beer) {\n      store.commit(\"removeQtdeProd\", beer);\n    },\n    deleteBeer: function deleteBeer(beer) {\n      store.commit(\"deleteBeer\", beer);\n    },\n    updateDiscount: function updateDiscount() {\n      if (this.totalBeers >= 10) {\n        this.discount = this.totalBeersPrice / 100 * 10;\n      } else {\n        this.discount = 0;\n      }\n    }\n  },\n  watch: {\n    totalBeers: function totalBeers() {\n      this.updateDiscount();\n    }\n  }\n};",{"version":3,"sources":["Checkout.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8EA,OAAA,KAAA,MAAA,iBAAA;AAEA,eAAA;AACA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA,KAHA;AAIA,IAAA,eAJA,6BAIA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CACA,UAAA,GAAA,EAAA,IAAA;AAAA,eAAA,GAAA,GAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,QAAA;AAAA,OADA,EAEA,CAFA,CAAA;AAIA,KATA;AAUA,IAAA,UAVA,wBAUA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA,IAAA;AAAA,eAAA,GAAA,GAAA,IAAA,CAAA,QAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA;AAZA,GADA;AAeA,EAAA,IAfA,kBAeA;AACA,WAAA;AACA,MAAA,QAAA,EAAA;AADA,KAAA;AAGA,GAnBA;AAoBA,EAAA,OApBA,qBAoBA;AACA,SAAA,aAAA;AACA,GAtBA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,IADA,EACA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,aAAA,EAAA,IAAA;AACA,KAHA;AAIA,IAAA,cAJA,0BAIA,IAJA,EAIA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,IAAA;AACA,KANA;AAOA,IAAA,UAPA,sBAOA,IAPA,EAOA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,YAAA,EAAA,IAAA;AACA,KATA;AAUA,IAAA,cAVA,4BAUA;AACA,UAAA,KAAA,UAAA,IAAA,EAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA,eAAA,GAAA,GAAA,GAAA,EAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,GAAA,CAAA;AACA;AACA;AAhBA,GAvBA;AAyCA,EAAA,KAAA,EAAA;AACA,IAAA,UAAA,EAAA,sBAAA;AACA,WAAA,cAAA;AACA;AAHA;AAzCA,CAAA","sourcesContent":["<template>\n\n<v-container>\n  <v-list two-line subheader>\n    <v-subheader inset>Checkout</v-subheader>\n\n    <v-list-tile\n      v-for=\"beer in beers\"\n      :key=\"beer.id\"\n      avatar\n    >\n      <v-list-tile-avatar>\n        <img :src=\"beer.image_url\">\n      </v-list-tile-avatar>\n      <v-list-tile-content>\n        <v-list-tile-title>{{ beer.name }}</v-list-tile-title>\n        <v-list-tile-sub-title>Qtde: \n      \n        <v-btn flat icon color=\"primary\" v-on:click=\"addQtdeProd(beer)\">\n          <v-icon>add</v-icon>\n        </v-btn>\n\n        {{ beer.quantity }}\n\n        <v-btn flat icon color=\"pink\" v-on:click=\"removeQtdeProd(beer)\">\n          <v-icon>remove</v-icon>\n        </v-btn>\n\n      </v-list-tile-sub-title>\n      </v-list-tile-content>\n\n      <v-list-tile-action>\n        <v-btn icon ripple>\n          <v-icon color=\"grey lighten-1\" v-on:click=\"deleteBeer(beer)\">delete_forever</v-icon>\n        </v-btn>\n      </v-list-tile-action>\n\n    </v-list-tile>\n\n    <v-divider inset></v-divider>\n\n    <v-alert :value=\"true\" type=\"success\">\n      R$: {{totalBeersPrice}}\n    </v-alert>\n\n    <v-btn flat color=\"teal\">\n        <v-icon>shopping_cart</v-icon>\n        <span>Finalizar pedido</span>\n    </v-btn>\n\n    <v-btn flat color=\"orange\" @click=\"$router.go(-1)\">Voltar</v-btn>\n\n  </v-list>   \n     \n  <v-list three-line subheader>\n\n    <span class=\"price\"> R$ {{(totalBeersPrice - discount).toFixed(2).replace('.',',')}} </span>\n    <br>\n\n    <span class=\"discount\" v-if=\"discount > 0\"> R$ {{totalBeersPrice.toFixed(2).replace('.',',')}}  </span>\n    <v-chip v-if=\"discount > 0\" label color=\"success\" text-color=\"white\"> 10% OFF </v-chip>\n\n  </v-list>\n\n  </v-container>\n</template>\n\n<style scoped>\n.discount {\n  text-decoration: line-through;\n  color: darkgray;\n}\n.price {\n  font-weight: bold;\n}\n</style>\n\n<script>\nimport store from \"@/store/cart.js\";\n\nexport default {\n  computed: {\n    beers() {\n      return store.state.beers;\n    },\n    totalBeersPrice() {\n      return store.state.beers.reduce(\n        (acc, item) => acc + item.price * item.quantity,\n        0\n      );\n    },\n    totalBeers() {\n      return store.state.beers.reduce((acc, item) => acc + item.quantity, 0);\n    }\n  },\n  data() {\n    return {\n      discount: 0\n    };\n  },\n  created() {\n    this.pdateDiscount();\n  },\n  methods: {\n    addQtdeProd(beer) {\n      store.commit(\"addQtdeProd\", beer);\n    },\n    removeQtdeProd(beer) {\n      store.commit(\"removeQtdeProd\", beer);\n    },\n    deleteBeer(beer) {\n      store.commit(\"deleteBeer\", beer);\n    },\n    updateDiscount() {\n      if (this.totalBeers >= 10) {\n        this.discount = (this.totalBeersPrice / 100) * 10;\n      } else {\n        this.discount = 0;\n      }\n    }\n  },\n  watch: {\n    totalBeers: function() {\n      this.updateDiscount();\n    }\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}